{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/static-build"
      },
      {
        "src": "client/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "client/build/static/$1"
      }
    ]
  }